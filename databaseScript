-- Table: public.calification

-- DROP TABLE IF EXISTS public.calification;

CREATE TABLE IF NOT EXISTS public.calification
(
    "ID_calification" integer NOT NULL GENERATED BY DEFAULT AS IDENTITY ( INCREMENT 1 START 1 MINVALUE 1 MAXVALUE 2147483647 CACHE 1 ),
    "user_ID" numeric NOT NULL,
    rate smallint,
    "item_ID" numeric NOT NULL,
    date date NOT NULL,
    CONSTRAINT calification_pkey PRIMARY KEY ("ID_calification")
)

TABLESPACE pg_default;

ALTER TABLE IF EXISTS public.calification
    OWNER to postgres;